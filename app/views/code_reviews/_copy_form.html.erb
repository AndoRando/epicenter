<h2>Copy existing code review</h2>
<%= form_for code_review, url: copy_code_review_index_path, html: { method: :post } do |f| %>
  <%= grouped_collection_select(:code_review, :id, Cohort.all, :code_reviews, :description, :id, :title) %>

  <div class="submit">
    <%= f.submit 'Copy', class: 'btn btn-warning' %>
  </div>
<% end %>
